@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.TextPage>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}

<div class="main-content body one-column cf">

	<h1>@Umbraco.Field("pageHeading")</h1>
	

	<h2>@Umbraco.Field("subHeading")</h2>

	<p><br /></p>

	<h3>@Umbraco.Field("sectionHeading")</h3>

    @Html.GetGridHtml(Model.Content, "mainText")
    

</div><!-- /.main-content -->